{
  "name": "gf-react-ui",
  "version": "1.0.0",
  "description": "一个基于React开发的组件库，包含基础组件、业务组件、区块、hooks、utils等",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "test": "father test",
    "test:coverage": "father test --coverage",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "format": "eslint --fix 'src/**/*.{js,ts,tsx}'",
    "start": "father doc dev",
    "doc:build": "father doc build",
    "doc:deploy": "father doc deploy",
    "lib:build": "father build && node ./scripts/moveDeclare.js && node ./scripts/changeLess2Css.js"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "father": "^2.29.2",
    "fs-extra": "^9.0.0",
    "klaw-sync": "^6.0.0",
    "prettier": "^2.0.2",
    "prettier-eslint": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gao525280067/gf-ui.git"
  },
  "keywords": [
    "react",
    "ui",
    "hooks"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "chengaofeng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gao525280067/gf-ui/issues"
  },
  "homepage": "https://github.com/gao525280067/gf-ui#readme",
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ],
  "sideEffects": false
}
